{
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        },
        {
            "src": "AI-Model/main.py",
            "use": "@vercel/python"
        },
        {
            "src": "AI-Model/main2.py",
            "use": "@vercel/python"
          }
    ],
    "routes": [
        {
            "src": "/api/main2",
            "dest": "AI-Model/main2.py"
          },
          {
            "src": "/api/main",
            "dest": "AI-Model/main.py"
          },
          {
            "src": "/(.*)",
            "dest": "index.js"
          }
    ]
}